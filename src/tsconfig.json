{
    "version": "1.8.10",
    "compilerOptions": {
        "jsx": "react",
        "target": "ES5",
        "outDir": "../lib",
        "module": "commonjs",
        "newLine": "LF",
        "declaration": true,
        "diagnostics": true,
        "emitDecoratorMetadata": true,
        "listFiles": true,
        "noImplicitAny": true,
        "noEmitOnError": false,
        "noImplicitReturns": true,
        "noImplicitUseStrict": false,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noFallthroughCasesInSwitch": true,
        "preserveConstEnums": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "files": [
        "./BalloonView.tsx",
        "./CanvasRenderView.tsx",
        "./ContextMenuView.tsx",
        "./DesktopView.tsx",
        "./Event.d.ts",
        "./GhostView.tsx",
        "./index.tsx",
        "./LayerSetView.tsx",
        "./LayerView.tsx",
        "./require.d.ts",
        "./ScopeView.tsx",
        "./SurfaceView.tsx",
        "./View.tsx",
        "../typings/browser.d.ts"
    ]
}
